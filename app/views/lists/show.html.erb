<h1><%= @list.name %></h1>
<%= form_with(model: [@list, Bookmark.new]) do |form| %>
  <%= form.label :movie_id, "Movie" %>
  <%= form.collection_select :movie_id, Movie.all, :id, :title %>
  <%= form.label :comment, "Comment" %>
  <%= form.text_area :comment %>
  <%= form.submit "Add Bookmark" %>
<% end %>
